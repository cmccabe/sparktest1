#!/bin/bash -x

export MYDIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
source "${MYDIR}/submit-common.sh"

./bin/spark-submit \
    --class com.cloudera.sparktest.SparkTest1 \
    --master yarn-cluster \
    --num-executors 1 \
    ${MYDIR}/../target/sparktest1-1.0-SNAPSHOT.jar \
    ${@}
